{
  "name": "bruery/platform",
  "type": "library",
  "description": "Bruery, BAP, Platform",
  "keywords": ["Symfony2", "Bruery", "BAP", "Platform"],
  "homepage": "https://bruery.github.io/platform/",
  "license": "MIT",
  "authors": [
    {
      "name": "viktore zara",
      "email": "viktore.zara@gmail.com",
      "homepage": "https://github.com/gzilt"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.3.3",

    "symfony/symfony":  "^2.8 || ^3.0",
    
    "sonata-project/classification-bundle": "^3.0",
    "sonata-project/formatter-bundle": "^3.0",
    "sonata-project/media-bundle": "^3.0",
    "sonata-project/timeline-bundle": "^3.0",

    "bruery/platform-core" : "dev-master",

    "simplethings/entity-audit-bundle": "~0.8",
    "snc/redis-bundle": "^2.0"
  },
  "replace": {
    "bruery/classification-bundle": "self.version",
    "bruery/formatter-bundle": "self.version",
    "bruery/media-bundle": "self.version",
    "bruery/timeline-bundle": "self.version",
    "bruery/oauth-server-bundle": "self.version",
    "bruery/entity-audit-bundle": "self.version"
  },
  "autoload": {
    "psr-4": { "Bruery\\": "src/bundles/" }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
